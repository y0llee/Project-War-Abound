	}
	new buffer[512], name[MAX_PLAYER_NAME];
	GetPlayerName(playerid, name, sizeof(name));
	if(pInfo[playerid][Admin] < 1)
	{
	    format(buffer, sizeof(buffer), "{FFFFFF}[Игрок] %s: %s", name, text);
	    SendClientMessageToAll(-1, buffer);
	    print(buffer);
	}
	else
	{
	    format(buffer, sizeof(buffer), "{6EF83C}[Администратор]{FFFFFF} %s: %s", name, text);
	    SendClientMessageToAll(-1, buffer);
	    print(buffer);
	}
	SetPVarInt(playerid, "AntiFlood", gettime() + 2);
	SetPlayerChatBubble(playerid, text, -1, 50.0, 6000);
	return 0;
}

