public OnPlayerDeath(playerid, killerid, reason)
{
	if(DuelGaming[playerid] == true)
	{
	
	    SetPlayerVirtualWorld(playerid, 0);
	    DuelGaming[playerid] = false;
	    DuelGaming[DuelRequest[playerid]] = false;
	    
	    SetPlayerVirtualWorld(DuelRequest[playerid], 0);
	    
	    if(GetPlayerTeam(DuelRequest[playerid]) == 2)
	    {
	        SetPlayerInterior(DuelRequest[playerid], 5);
	        SetPlayerPos(DuelRequest[playerid], 323.8701, 1128.9177, 1083.8828);
	    }
	    else
	    {
	        SetPlayerInterior(DuelRequest[playerid], 0);
			SetPlayerPos(DuelRequest[playerid], 242.5319, 1865.3909, 11.4609);
	    }
	    
	    SetPlayerHealth(DuelRequest[playerid], 100);
	    
     	ResetPlayerWeapons(DuelRequest[playerid]);
     	
     	SendClientMessage(killerid, -1, "{6EF83C}War Abound:{FFFFFF} Вы победили в дуэли!");
     	SendClientMessage(playerid, -1, "{6EF83C}War Abound:{FFFFFF} Вы проиграли в дуэли.");

		GameTextForPlayer(killerid, "~g~Win!", 2000, 6);
	    GameTextForPlayer(playerid, "~r~Lose!", 2000, 6);
	    
	    GivePlayerMoneyNoCheat(killerid, DuelCost[killerid]);
	    GivePlayerMoneyNoCheat(playerid, -DuelCost[playerid]);
	    
	    SetPlayerArmour(playerid, 0);
		SetPlayerArmour(DuelRequest[playerid], 0);
	    
	    DuelCost[killerid] = 0;
	    DuelCost[playerid] = 0;
	    
	    DuelRequest[DuelRequest[playerid]] = -1;
	    DuelRequest[playerid] = -1;
	}
	else
	{
		if(playerid == PikapPdb)
		{
		    pikapi[2] = CreatePickup(1210, 23, 249.7421, 1855.6976, 8.7613, -1);
		    PikapPdb = -1;
		    SendClientMessageToAll(-1, "{6EF83C}War Abound:{FFFFFF} Кейс спасён {6EF83C}Армией{FFFFFF}.");
			SetPlayerScore(killerid, GetPlayerScore(killerid) + 10);
			GameTextForPlayer(killerid, "~g~+10 Score", 2000, 6);
			Delete3DTextLabel(Case3d);
		}
		if(GetPlayerTeam(playerid) != GetPlayerTeam(killerid))
		{
		    pInfo[playerid][Armour] = 0;
		    pInfo[playerid][Deaths]++;
		    if(pInfo[playerid][Cash] >= 200)
			{
			    GivePlayerMoneyNoCheat(playerid, -200);
		  	}
		    if(GetPlayerScore(playerid) >= 1)
		    {
			    SetPlayerScore(playerid, GetPlayerScore(playerid) - 1);
		    }
		   	if(killerid != INVALID_PLAYER_ID)
			{
			    SetPlayerScore(killerid, GetPlayerScore(killerid) + 1);
			    GivePlayerMoneyNoCheat(playerid, 200);
			    pInfo[killerid][Kills]++;
			    SendDeathMessage(killerid, playerid, reason);
		    }
	    }
	    else
	    {
	        ShowPlayerDialog(killerid, 666, DIALOG_STYLE_MSGBOX, "Уведомление", "{FFFFFF}Вы были кикнуты за убийство товарища!", "Готово", "");
	       	SetTimerEx("DelayedKick", 1000, false, "i", killerid);
		}
	}
	return 1;
}

