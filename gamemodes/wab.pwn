forward public OnPlayerRegister(playerid);
public OnPlayerRegister(playerid)
{
	SendClientMessage(playerid, -1, "{6EF83C}War Abound:{FFFFFF} Вы зарегистрировались и автоматически авторизовались!");
    pInfo[playerid][LoggedIn] = true;
    if(Antibot[playerid] == false)
	{
		pInfo[playerid][Rnd] = random(5);
		if(pInfo[playerid][Rnd] == 0)
		{
			pInfo[playerid][Abtxt] = "QsyBy";
		}
		if(pInfo[playerid][Rnd] == 1)
		{
			pInfo[playerid][Abtxt] = "qMZAq";
		}
		if(pInfo[playerid][Rnd] == 2)
		{
			pInfo[playerid][Abtxt] = "bitSi";
		}
		if(pInfo[playerid][Rnd] == 3)
		{
			pInfo[playerid][Abtxt] = "Hvkgv";
		}
		if(pInfo[playerid][Rnd] == 4)
		{
			pInfo[playerid][Abtxt] = "ZgRSf";
		}

		new buffer[128];
		format(buffer, sizeof(buffer), "{FFFFFF} Введите данный текст: {6EF83C}%s{FFFFFF}.", pInfo[playerid][Abtxt]);
		ShowPlayerDialog(playerid, 2, DIALOG_STYLE_INPUT, "{6EF83C}Анти-Бот", buffer, "Готово", "Отмена");

	}
	SendClientMessage(playerid, -1, "{6EF83C}War Abound:{FFFFFF} Вы зарегистрировались и автоматически авторизовались!");
	new ipka[16];
	GetPlayerIp(playerid, ipka, sizeof(ipka));
	pInfo[playerid][IP] = ipka;
	GivePlayerMoneyNoCheat(playerid, 5000);
	return 1;
}

