forward public OnPlayerDataCheck(playerid, corrupt_check);
public OnPlayerDataCheck(playerid, corrupt_check)
{
	if (corrupt_check != Corrupt_Check[playerid]) return Kick(playerid);

	new String[150];

	if(cache_num_rows() > 0)
	{

		cache_get_value(0, "PASSWORD", pInfo[playerid][Password], 65);
		cache_get_value(0, "SALT", pInfo[playerid][Salt], 11);

		pInfo[playerid][Player_Cache] = cache_save();

		format(String, sizeof(String), "{FFFFFF}Привет, %s.\n\n{6EF83C}Этот аккаунт зарегистрирован.\n\
		{6EF83C}Пожалуйста, вспомните пароль и введите его.\n\n", pInfo[playerid][Name]);
		ShowPlayerDialog(playerid, DIALOG_LOGIN, DIALOG_STYLE_PASSWORD, "{6EF83C}War Abound:{FFFFFF} Авторизация", String, "Готово", "Отмена");
	}
	else
	{
		format(String, sizeof(String), "{FFFFFF}Привет, %s.\n\n{6EF83C}Этот аккаунт не зарегистрирован.\n\
		{6EF83C}Пожалуйста, придумайте пароль и введите его.\n\n", pInfo[playerid][Name]);
		ShowPlayerDialog(playerid, DIALOG_REGISTER, DIALOG_STYLE_INPUT, "{6EF83C}War Abound:{FFFFFF} Регистрация", String, "Готово", "Отмена");
	}
	return 1;
}

