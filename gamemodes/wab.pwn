	new Scarca = CreateVehicle(carid, X, Y, Z, Angle, -1, -1, -1);
 	PutPlayerInVehicle(playerid, Scarca, 0);
	printf("Игрок %s зареспавнил транспорт(%d)", name, carid);
	return 1;
}

