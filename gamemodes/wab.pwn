public OnPlayerConnect(playerid)
{

    DuelRequest[playerid] = -1;
	DuelGaming[playerid] = false;
    DuelOutRequest[playerid] = -1;
    DuelCost[playerid] = 0;
    
	damage[playerid][0] = TextDrawCreate(137.500000, 349.416625, "Nick - Weapon +damage");
    TextDrawLetterSize(damage[playerid][0], 0.226874, 0.859166);
    TextDrawAlignment(damage[playerid][0], 1);
    TextDrawColor(damage[playerid][0], 16711935);
    TextDrawSetShadow(damage[playerid][0], 0);
    TextDrawSetOutline(damage[playerid][0], 1);
    TextDrawBackgroundColor(damage[playerid][0], 51);
    TextDrawFont(damage[playerid][0], 1);
    TextDrawSetProportional(damage[playerid][0], 1);

    damage[playerid][1] = TextDrawCreate(448.500000, 346.333251, "Nick - Weapon -damage");
    TextDrawLetterSize(damage[playerid][1], 0.209374, 0.934999);
    TextDrawAlignment(damage[playerid][1], 1);
    TextDrawColor(damage[playerid][1], 16777215);
    TextDrawSetShadow(damage[playerid][1], 0);
    TextDrawSetOutline(damage[playerid][1], 1);
    TextDrawBackgroundColor(damage[playerid][1], 51);
    TextDrawFont(damage[playerid][1], 1);
    TextDrawSetProportional(damage[playerid][1], 1);

    GangZoneShowForAll(GZ_ZONE1, 0x00800096);
	GangZoneShowForAll(GZ_ZONE2, 0x80000096);

 	SetPlayerMapIcon(playerid, 1, -89.2680, 1378.3103, 10.4698, 29, 0, MAPICON_GLOBAL);

   	RemoveBuildingForPlayer(playerid, 16094,191.1406,1870.0391,21.4766,0.25);
    RemoveBuildingForPlayer(playerid, 1411, 347.1953, 1799.2656, 18.7578, 0.25);
    RemoveBuildingForPlayer(playerid, 1411, 342.9375, 1796.2891, 18.7578, 0.25);

    Antibot[playerid] = false;
	ResetPlayerMoney(playerid);
	new DB_Query[115];

	pInfo[playerid][Kills] = 0;
	pInfo[playerid][Deaths] = 0;
	pInfo[playerid][PasswordFails] = 0;
	AutAdm[playerid] = false;
	SetPlayerTeam(playerid, NO_TEAM);
	Question[playerid] = false;
	scar[playerid] = false;

	GetPlayerName(playerid, pInfo[playerid][Name], MAX_PLAYER_NAME);
	Corrupt_Check[playerid]++;

	new ipka[16];
	GetPlayerIp(playerid, ipka, sizeof(ipka));

	mysql_format(Database, DB_Query, sizeof(DB_Query), "SELECT * FROM `PLAYERS` WHERE `USERNAME` = '%e' LIMIT 1", pInfo[playerid][Name]);
	mysql_tquery(Database, DB_Query, "OnPlayerDataCheck", "ii", playerid, Corrupt_Check[playerid]);

	AuthTimer[playerid] = SetTimerEx("TimingAuth", 40000, false, "i", playerid);

	Question[playerid] = false;
	PlayAudioStreamForPlayer(playerid, "https://bit.ly/2HZ7QoH");
	GivePlayerMoneyNoCheat(playerid, 5000);
    TextDrawShowForPlayer(playerid, Logo);
	new buffer[128], name[MAX_PLAYER_NAME];
	GetPlayerName(playerid, name, sizeof(name));
	format(buffer, sizeof(buffer), "{6EF83C}War Abound:{FFFFFF} Игрок {6EF83C}%s{FFFFFF} присоединился на сервер.", name);
	SendClientMessageToAll(-1, buffer);
	printf("Игрок %s[%d] зашёл с IP адреса: %s. (%s, %s)", name, playerid, ipka, GetPlayerCountry(playerid), GetPlayerCity(playerid));

	return 1;
}

public OnPlayerDisconnect(playerid, reason)
{
	// удаление маппинга
    RemoveBuildingForPlayer(playerid, 3279, 262.0938, 1807.6719, 16.8203, 0.25);
	RemoveBuildingForPlayer(playerid, 1411, 347.1953, 1799.2656, 18.7578, 0.25);
	RemoveBuildingForPlayer(playerid, 1411, 342.9375, 1796.2891, 18.7578, 0.25);
	RemoveBuildingForPlayer(playerid, 16670, 330.7891, 1813.2188, 17.8281, 0.25);
	RemoveBuildingForPlayer(playerid, 3279, 113.3828, 1814.4531, 16.8203, 0.25);
	RemoveBuildingForPlayer(playerid, 16095, 279.1328, 1829.7813, 16.6328, 0.25);
	RemoveBuildingForPlayer(playerid, 16094, 191.1406, 1870.0391, 21.4766, 0.25);
	RemoveBuildingForPlayer(playerid, 3279, 103.8906, 1901.1016, 16.8203, 0.25);
	RemoveBuildingForPlayer(playerid, 16096, 120.5078, 1934.0313, 19.8281, 0.25);
	RemoveBuildingForPlayer(playerid, 16671, 193.9531, 2051.7969, 20.1797, 0.25);
	RemoveBuildingForPlayer(playerid, 3279, 233.4297, 1934.8438, 16.8203, 0.25);
	RemoveBuildingForPlayer(playerid, 3279, 267.0625, 1895.2969, 16.8203, 0.25);
	RemoveBuildingForPlayer(playerid, 16668, 357.9375, 2049.4219, 16.8438, 0.25);
	RemoveBuildingForPlayer(playerid, 16669, 380.2578, 1914.9609, 17.4297, 0.25);

	Corrupt_Check[playerid]++;

	if(DuelGaming[playerid] == true)
	{

	    SetPlayerVirtualWorld(DuelRequest[playerid], 0);
	    DuelGaming[playerid] = false;
	    DuelGaming[DuelRequest[playerid]] = false;

	    SetPlayerVirtualWorld(DuelRequest[playerid], 0);

	    if(GetPlayerTeam(DuelRequest[playerid]) == 2)
	    {
	        SetPlayerInterior(DuelRequest[playerid], 5);
	        SetPlayerPos(DuelRequest[playerid], 323.8701, 1128.9177, 1083.8828);
	    }
	    else
	    {
	        SetPlayerInterior(DuelRequest[playerid], 0);
			SetPlayerPos(DuelRequest[playerid], 242.5319, 1865.3909, 11.4609);
	    }

	    SetPlayerHealth(DuelRequest[playerid], 100);
	    
        ResetPlayerWeapons(DuelRequest[playerid]);
	    
	    GivePlayerMoneyNoCheat(playerid, -DuelCost[playerid]);
	    GivePlayerMoneyNoCheat(playerid, DuelCost[DuelRequest[playerid]]);
	    
	    DuelRequest[DuelRequest[playerid]] = -1;
		SetPlayerArmour(DuelRequest[playerid], 0);
	    SendClientMessage(DuelRequest[playerid], -1, "{6EF83C}War Abound:{FFFFFF} Дуэль прервана. Игрок вышел.");
		SendClientMessage(DuelRequest[playerid], -1, "{6EF83C}War Abound:{FFFFFF} Вам выдана компенсация.");
	}
	if(DuelRequest[playerid] != -1)
	{
	    DuelRequest[DuelRequest[playerid]] = -1;
	    DuelRequest[playerid] = -1;
	    SendClientMessage(DuelRequest[playerid], -1, "{6EF83C}War Abound:{FFFFFF} Запрос отменен. Игрок вышел.");
	}

	new DB_Query[256];
 	pInfo[playerid][Cash] = GetPlayerMoney(playerid);
	pInfo[playerid][Score] = GetPlayerScore(playerid);

	mysql_format(Database, DB_Query, sizeof(DB_Query), "UPDATE `PLAYERS` SET `IP` = '%s', `SCORE` = %d, `CASH` = %d, `KILLS` = %d, `ADMIN` = %d, `VIP` = %d, `ARMOUR` = %d, `KVEST6UB` = %d, `CAR` = %d, `DCOINS` = %d, `DEATHS` = %d WHERE `ID` = %d LIMIT 1",
 	pInfo[playerid][IP], pInfo[playerid][Score], pInfo[playerid][Cash], pInfo[playerid][Kills], pInfo[playerid][Admin], pInfo[playerid][Vip], pInfo[playerid][Armour], pInfo[playerid][Kvest6UB], pInfo[playerid][Car], pInfo[playerid][Dcoins], pInfo[playerid][Deaths], pInfo[playerid][ID]);

	mysql_tquery(Database, DB_Query);

	if(cache_is_valid(pInfo[playerid][Player_Cache]))
	{
		cache_delete(pInfo[playerid][Player_Cache]);
		pInfo[playerid][Player_Cache] = MYSQL_INVALID_CACHE;
	}

    KillTimer(AuthTimer[playerid]);

	pInfo[playerid][LoggedIn] = false;
	print("OnPlayerDisconnect has been called.");

	if(playerid == PikapPdb)
	{
	    pikapi[2] = CreatePickup(1210, 23, 249.7421, 1855.6976, 8.7613, -1);
        PikapPdb = -1;
		SendClientMessageToAll(-1, "{6EF83C}War Abound:{FFFFFF} Кейс был возвращён {6EF83C}Армии{FFFFFF}, игрок который забрал кейс покинул сервер.");
        Delete3DTextLabel(Case3d);
	}

	new buffer[128], name[MAX_PLAYER_NAME];
	GetPlayerName(playerid, name, sizeof(name));
	format(buffer, sizeof(buffer), "{6EF83C}War Abound:{FFFFFF} Игрок {6EF83C}%s{FFFFFF} покинул сервер.", name);
	SendClientMessageToAll(-1, buffer);
	return 1;
}

public OnPlayerSpawn(playerid)
